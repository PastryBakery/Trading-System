<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.shape.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import com.jfoenix.controls.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<StackPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <GridPane fx:id="center">
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </GridPane>
      <BorderPane prefHeight="800.0" prefWidth="1000.0">
         <center>
            <JFXDrawer fx:id="drawer" defaultDrawerSize="250.0" layoutY="-6.0" prefWidth="250.0" />
         </center>
         <top>
            <JFXToolbar minHeight="60.0" minWidth="1000.0" BorderPane.alignment="CENTER">
               <center>
                  <HBox style="-fx-background-color: #ba8baf;" BorderPane.alignment="CENTER">
                     <padding>
                        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                     </padding>
                     <children>
                        <JFXHamburger fx:id="hamburger" />
                     </children>
                  </HBox>
               </center>
            </JFXToolbar>
         </top>
      </BorderPane>
   </children>
</StackPane>
