<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import com.jfoenix.controls.JFXTextField?>
<HBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
      fx:id="meeting" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <VBox>
            <children>
                <HBox fx:id="dateTime" alignment="BOTTOM_CENTER" spacing="20.0">
                    <padding>
                        <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                    </padding>
                    <children>
                        <TextField fx:id="year" promptText="YEAR" />
                        <TextField fx:id="month" promptText="MONTH" />
                        <TextField fx:id="day" promptText="DAY" />
                        <TextField fx:id="hour" promptText="HOUR" />
                        <TextField fx:id="minute" promptText="MINUTE" />
                    </children>
                </HBox>
                <HBox prefHeight="100.0" prefWidth="200.0" spacing="20.0">
                    <padding>
                        <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                    </padding>
                    <children>
                        <TextField fx:id="street" prefHeight="25.0" prefWidth="334.0" promptText="STREET" />
                        <TextField fx:id="streetNum" prefHeight="25.0" prefWidth="269.0" promptText="STREET NUMBER" />
                        <TextField fx:id="city" prefHeight="25.0" prefWidth="323.0" promptText="CITY" />
                        <TextField fx:id="country" prefHeight="25.0" prefWidth="356.0" promptText="COUNTRY" />
                    </children>
                </HBox>
            </children>
        </VBox>
    </children>
</HBox>
