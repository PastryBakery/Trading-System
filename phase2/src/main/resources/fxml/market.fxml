<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <TableView fx:id="trades" blendMode="DARKEN" opacity="0.8" prefHeight="230.0" prefWidth="600.0" />
        <TabPane prefHeight="160.0" prefWidth="600.0" tabClosingPolicy="UNAVAILABLE">
            <tabs>
                <Tab text="One-Way-Trade">
                    <content>
                        <VBox>
                            <children>
                                <HBox alignment="BOTTOM_CENTER" spacing="20.0">
                                    <padding>
                                        <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                                    </padding>
                                    <children>
                                        <TextField fx:id="items" prefHeight="25.0" prefWidth="393.0" promptText="WHAT ITEMS DO YOU WANT?" />
                                        <TextField fx:id="traders" prefHeight="25.0" prefWidth="373.0" promptText="WHO DO YOU WANT TO TRADE WITH?" />
                                    </children>
                                </HBox>
                                <HBox fx:id="dateTime" alignment="BOTTOM_CENTER" spacing="20.0">
                                    <padding>
                                        <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                                    </padding>
                                    <children>
                                        <TextField fx:id="year" promptText="YEAR" />
                                        <TextField fx:id="month" promptText="MONTH" />
                                        <TextField fx:id="day" promptText="DAY" />
                                        <TextField fx:id="hour" promptText="HOUR" />
                                        <TextField fx:id="minute" promptText="MINUTE" />
                                    </children>
                                </HBox>
                                <HBox prefHeight="100.0" prefWidth="200.0" spacing="20.0">
                                    <padding>
                                        <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                                    </padding>
                                    <children>
                                        <TextField fx:id="street" promptText="STREET" />
                                        <TextField fx:id="streetNum" promptText="STREET NUMBER" />
                                        <TextField fx:id="city" promptText="CITY" />
                                        <TextField fx:id="country" promptText="COUNTRY" />
                                    </children>
                                </HBox>
                                <ButtonBar prefHeight="40.0" prefWidth="200.0" />
                                  <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="616.0" text="Button">
                           <effect>
                              <ColorInput paint="#a3ffe8" />
                           </effect></Button>
                            </children>
                        </VBox>
                    </content>
                </Tab>
                <Tab text="Two-Way-Trade">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                    </content>
                </Tab>
                <Tab text="Three-Way-Trade">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                    </content>
                </Tab>
            </tabs>
        </TabPane>
    </children>
</VBox>
